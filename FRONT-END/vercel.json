{
  "version": 2,
  "builds": [
    {
      "src": "dist/**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/assets/(.*)\\.js",
      "headers": {
        "Content-Type": "application/javascript"
      },
      "dest": "/assets/$1.js"
    },
    {
      "src": "/(.*)\\.js",
      "headers": {
        "Content-Type": "application/javascript"
      },
      "dest": "/$1.js"
    },
    {
      "src": "/(.*)\\.css",
      "headers": {
        "Content-Type": "text/css"
      },
      "dest": "/$1.css"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}
